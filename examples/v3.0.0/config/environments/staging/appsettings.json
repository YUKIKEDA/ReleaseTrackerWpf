{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=staging-db.myapp.com;Database=MyApp_Staging;User Id=staging_user;Password=staging_password;MultipleActiveResultSets=true",
    "Redis": "staging-redis.myapp.com:6379"
  },
  "AppSettings": {
    "Environment": "Staging",
    "ApiBaseUrl": "https://api-staging.myapp.com",
    "EnableSwagger": false,
    "EnableDetailedErrors": false,
    "CacheTimeoutMinutes": 15
  },
  "ExternalServices": {
    "PaymentGateway": {
      "BaseUrl": "https://staging-payments.example.com",
      "ApiKey": "staging_api_key_54321",
      "ClientVersion": "1.0.0",
      "TimeoutSeconds": 30
    },
    "EmailService": {
      "BaseUrl": "https://staging-email.example.com",
      "ApiKey": "staging_email_key_09876",
      "ClientId": "staging_client_002",
      "TimeoutSeconds": 15
    }
  },
  "Database": {
    "CommandTimeout": 30,
    "EnableRetryOnFailure": true,
    "MaxRetryCount": 3,
    "EnableSensitiveDataLogging": false
  },
  "Security": {
    "JwtSecret": "staging_jwt_secret_key_very_long_and_secure",
    "JwtExpiryMinutes": 120,
    "RequireHttps": true,
    "AllowedOrigins": [
      "https://staging.myapp.com",
      "https://staging-admin.myapp.com"
    ]
  },
  "Features": {
    "EnableUserRegistration": true,
    "EnableEmailVerification": true,
    "EnableTwoFactorAuth": false,
    "MaxFileUploadSizeMB": 25,
    "AllowedFileTypes": [".jpg", ".png", ".pdf", ".doc", ".docx", ".xlsx"]
  }
}
